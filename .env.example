# Description: Template for environment variables required by the LMDP MSK Pipeline
# Description: Copy to .env and fill in values; never commit .env to source control

# AWS
CDK_DEFAULT_ACCOUNT=497495481268
CDK_DEFAULT_REGION=us-east-1

# Snowflake (provisioned via AWS Marketplace)
SNOWFLAKE_ACCOUNT_URL=https://<org>-<account>.snowflakecomputing.com
SNOWFLAKE_DATABASE=LM_METRICS
SNOWFLAKE_SCHEMA=PIPELINE
SNOWFLAKE_TABLE=ROW_EVENTS
SNOWFLAKE_WAREHOUSE=LM_FIREHOSE_WH
SNOWFLAKE_USER=LM_FIREHOSE_SVC

# MSK
MSK_CLUSTER_ARN=arn:aws:kafka:us-east-1:497495481268:cluster/<cluster-name>/<cluster-id>
MSK_VPC_ID=vpc-<id>
MSK_SECURITY_GROUP_ID=sg-<id>

# Firehose
FIREHOSE_STREAM_NAME=lm-datapublisher-delivery
BATCH_SIZE=500
